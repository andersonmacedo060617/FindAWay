@model FindAWay.Rules.Game

<div class="col col-md-6 col-lg-offset-3">
    <div class="row">
        <div class="col-md-12">
            <div class="alert alert-success" role="alert">
                <h4 align="center">Start</h4>
            </div>
        </div>
    </div>
    @for (int i = 0; i < Model.GameBoard.Steps; i++)
    {
        @Html.Action("LoadStep", "FindWay", new { step = i})
    }
    <div class="row">
        <div class="col-md-12">
            <div class="alert alert-success" role="alert">
                <h4 align="center">Finish</h4>
            </div>
        </div>
    </div>
</div>

